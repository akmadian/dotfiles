alias ll='ls -alF'
alias la='ls -A'
alias l='ls -CF'
alias fgui='nautilus .'
alias cls='clear'
alias um='umount'
alias g='git'
alias gc='git c'
alias gp='git push'

# Function Aliases
alias ao='audio-out'
alias nm='nas-mount'

# Add an "alert" alias for long running commands.  Use like so:
#   sleep 10; alert
alias alert='notify-send --urgency=low -i "$([ $? = 0 ] && echo terminal || echo error)" "$(history|tail -n1|sed -e '\''s/^\s*[0-9]\+\s*//;s/[;&|]\s*alert$//'\'')"'